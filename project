    print(str(n),"SSIM: %.2f" % (score))
    (score, diff) = compare_ssim(gray[44], gray[n], full=True,multichannel=False,gaussian_weights=True)
    print(str(n),"Gray SSIM: %.2f" % (score))

#check if images are overlapping

